<view class="weui-panel weui-panel_access">
    <view class="weui-panel__bd" bindtap="toDetail">
        <block wx:for-items="{{activityList}}" wx:key="{{index}}">
            <view class="weui-media-box weui-media-box_text">
                <view class="weui-media-box__title weui-media-box__title_in-text">{{activityList[index].title}}</view>
                <view class="weui-media-box__desc">{{activityList[index].content}}</view>
                <view class="weui-media-box__info">
                    <view class=" weui-media-box__info__meta">2017.3.22</view>
                    <view class="weui-media-box__info__meta weui-media-box__info__meta_extra">类型：税务</view>
                </view>
            </view>
        </block>
    </view>
</view>