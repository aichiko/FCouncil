.report_page {
    position: relative;
    height: 1000rpx
}
.doc-title {
    display: block;
    font-weight: 800;
    font-size: 36rpx;
    margin-top: 15rpx;
    text-align: center;
    margin-left: 20rpx;
    margin-right: 20rpx
}
.doc-part-content {
    position: relative;
    font-size: 32rpx;
    line-height: 36rpx;
    color: #3c3c3c;
    margin-top: 30rpx;
    text-indent: 2em;
    padding-left: 40rpx;
    padding-right: 40rpx;

    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:14;
}
.read-wholedoc {
    width: 85%;
    height: 88rpx;
    line-height: 88rpx;
    top: 50rpx;
    color:#ffffff;
    background-color: #137FC1;
    border-color: #ccc;
    transform: rotate(0deg);
    -webkit-transform: rotate(0deg);
    font-size: 32rpx;
    border-radius: 12rpx;
    border-width: 1rpx;
    border-style: solid;
    text-align: center;
    font-weight: normal;
    font-style: normal;
    z-index: 5;
    opacity: 1;
    pointer-events: auto
}