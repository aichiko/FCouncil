    border-color: #7cb342;
    transform: rotate(0deg);
    -webkit-transform: rotate(0deg);
    font-size: 14.0px;
    border-radius: 8px;
    border-width: 1px;
    border-style: solid;
    text-align: center;
    font-weight: normal;
    font-style: normal;
    z-index: 5;
    opacity: 1;
    pointer-events: auto
}