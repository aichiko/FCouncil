.page {
    position: relative;
}
.doc-title {
    display: block;
    font-weight: 800;
    font-size: 16px;
    margin-top: 5px;
    text-align: center;
}
.doc-part-content {
    position: relative;
    font-size: 14px;
    line-height: 16px;
    margin-top: 20px;
    text-indent: 2em;
    padding-left: 10px;
    padding-right: 10px;
    height: 225px;

    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:14;
}
